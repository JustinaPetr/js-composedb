{
  "name": "@composedb/database-service",
  "version": "0.4.0",
  "author": "3Box Labs",
  "license": "(Apache-2.0 OR MIT)",
  "homepage": "https://github.com/ceramicstudio/js-composedb#readme",
  "keywords": [
    "ceramic",
    "composedb"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ceramicstudio/js-composedb",
    "directory": "packages/database-service"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16"
  },
  "sideEffects": false,
  "scripts": {
    "build:clean": "del dist",
    "build:js": "swc src -d ./dist --config-file ../../.swcrc",
    "build:types": "tsc --emitDeclarationOnly --skipLibCheck",
    "build": "pnpm build:clean && pnpm build:types && pnpm build:js",
    "lint": "eslint src --fix",
    "prepare": "pnpm build",
    "prepublishOnly": "package-check"
  },
  "dependencies": {
    "@composedb/graph-codecs": "workspace:*",
    "@composedb/model-codecs": "workspace:*",
    "@composedb/services-rpc": "workspace:*",
    "@trpc/server": "^10.16.0",
    "fp-ts": "^2.13.1",
    "io-ts": "^2.2.20",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.1.6",
    "typeorm": "^0.3.12"
  },
  "devDependencies": {
    "json-schema-typed": "^8.0.1"
  }
}
